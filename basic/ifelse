xiaoming={"height":0.99,"weight":80.5}
height=xiaoming.get("height")
weight=xiaoming.get("weight")


if (weight / (height**2)) <=18.5:
    print ("过轻")
elif (weight / (height**2)) <=25:
    print ("正常")
elif (weight / (height**2)) <=28:
    print("过重")
elif (weight / (height**2)) <=32:
    print("肥胖")
else :
    print("严重肥胖")
